{
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_title": "Extension popup"
    },
    "background": {
        "service_worker": "js/worker.js"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": false
    },
    "permissions": ["activeTab", "scripting", "storage"]
}
